{
  "name": "Git Extension",
  "version": "0.0.0",
  "description": "Track repos and memes",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "js":["content_scripts.js"]
    }
  ],
  "manifest_version": 2
}